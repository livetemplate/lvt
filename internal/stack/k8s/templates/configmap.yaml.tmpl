apiVersion: v1
kind: ConfigMap
metadata:
  name: {{ .ProjectName }}-config
  namespace: {{ .Namespace }}
  labels:
    app: {{ .ProjectName }}
    managed-by: lvt
data:
  {{- if ne .Storage "none" }}
  storage-bucket: "your-bucket-name"
  storage-region: "us-east-1"
  {{- end }}
  # Add other non-sensitive configuration here
